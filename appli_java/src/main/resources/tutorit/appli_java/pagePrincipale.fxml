<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tutorit.appli_java.PagePrincipaleController">
   <children>
      <VBox layoutX="18.0" layoutY="16.0" prefHeight="367.0" prefWidth="1000.0">
         <children>
            <HBox prefHeight="27.0" prefWidth="1000.0">
               <children>
                  <Button fx:id="boutonDomaines" mnemonicParsing="false" onAction="#domaines" text="Domaines où je cherche des cours" />
                  <Button fx:id="boutonChercherTuteur" mnemonicParsing="false" onAction="#chercherTuteur" prefHeight="24.0" prefWidth="196.0" text="Rechercher un tuteur" />
                  <Button fx:id="boutonSessionsEleve" mnemonicParsing="false" onAction="#sessionsEleve" prefHeight="24.0" prefWidth="223.0" text="Sessions en tant qu'élève" />
               </children>
            </HBox>
            <HBox prefHeight="27.0" prefWidth="1000.0">
               <children>
                  <Button fx:id="boutonPrestations" mnemonicParsing="false" onAction="#prestations" prefHeight="24.0" prefWidth="132.0" text="Mes prestations" />
                  <Button fx:id="boutonDisponibilités" mnemonicParsing="false" onAction="#disponibilites" text="Mes disponibilités" />
                  <Button fx:id="boutonSessionsTuteur" mnemonicParsing="false" onAction="#sessionsTuteur" prefHeight="24.0" prefWidth="233.0" text="Sessions en tant que tuteur" />
               </children></HBox>
            <StackPane fx:id="contenu" prefHeight="257.0" prefWidth="657.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
